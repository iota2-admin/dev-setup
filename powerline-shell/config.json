{
  "segments": [
    "virtual_env",
    "username",
    "battery",
    "cwd",
    "git",
    "hg",
    "root"
  ],
  "mode": "patched",
  "cwd": {
    "max_depth": 5
  },
  "battery": {
    "always_show_percentage": "true"
  },
  "theme": "~/.config/powerline-shell/themes/gruvbox.py",
  "vcs": {
    "show_symbol": "true"
  }
}
